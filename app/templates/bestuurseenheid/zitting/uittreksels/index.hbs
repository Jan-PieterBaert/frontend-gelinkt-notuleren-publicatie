<div class="container-flex--scroll u-padding--round">
  <div class="grid">
    <div class="col--7-12 col--9-12--m col--1-1--s">
      <h1 class="h1 u-spacer--top--small">
        Bekendmakingen en uittreksels van {{get (await model.bestuursorgaan.isTijdsspecialisatieVan) 'naam'}}, zitting van {{moment-format model.geplandeStart 'D MMMM YYYY, HH:mm'}}
      </h1>
      <div class="grid grid--is-stacked">
        {{#each uittreksels as |uittreksel|}}
	      	<div class="col--12-12">
  	        {{uittreksel-overview-item uittreksel=uittreksel}}
		      </div>
        {{else}}
	      	<div class="col--12-12">
	          <p>Geen bekendmakingen of uittreksels beschikbaar.</p>
	        </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>

<div class="u-hidden"
     vocab="http://data.vlaanderen.be/ns/besluit#"
     prefix="lblodbp: http://data.lblod.info/vocabularies/besluitPublicatie/ eli: http://data.europa.eu/eli/ontology# prov: http://www.w3.org/ns/prov# mandaat: http://data.vlaanderen.be/ns/mandaat# besluit: http://data.vlaanderen.be/ns/besluit# person: http://www.w3.org/ns/person# persoon: http://data.vlaanderen.be/ns/persoon# "
     typeof="https://data.vlaanderen.be/ns/generiek#DocumentOnderdeel" resource="#">
<span property="http://purl.org/dc/terms/type" resource="lblodbp:UittrekselDocument"></span>
  <div resource="http://data.lblod.info/id/zittingen/{{model.id}}" typeof="besluit:Zitting">
      {{#each uittreksels as |uittreksel|}}
        <div property="ext:uittreksel" resource="http://data.lblod.info/id/uittreksels/{{uittreksel.id}}" typeof="ext:Uittreksel">
          {{uittreksel-overview-item uittreksel=uittreksel}}
        </div>
      {{else}}
        <div class="col--12-12">
          <p>Geen bekendmakingen of uittreksels beschikbaar.</p>
        </div>
      {{/each}}
  </div>
</div>

{{outlet}}

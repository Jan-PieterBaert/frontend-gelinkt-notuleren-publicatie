<table>
  <tr>
    <th>Naam</th>
    <th>Label</th>
  </tr>
  {{#each model as |bestuurseenheid|}}
    <tr>
      <td>
        <div>
          {{#link-to "bestuurseenheid" bestuurseenheid.id bestuurseenheid.classificatie.id}}
            {{bestuurseenheid.naam}}
          {{/link-to}}
        </div>
      </td>
      <td>
        <div>
          {{bestuurseenheid.classificatie.label}}
        </div>
      </td>
    </tr>
  {{/each}}
</table>

{{outlet}}
